<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <enterprise-beans>
        <session>
            <description>This EJB delegates clients' request to BLContext</description>
            <display-name>BLContext Proxy EJB</display-name>
            <ejb-name>BaseBLContext</ejb-name>
            <home>com.dream.bizsdk.ejb.IBLContextHome</home>
            <remote>com.dream.bizsdk.ejb.IBLContextRemote</remote>
            <ejb-class>com.dream.bizsdk.ejb.BaseBLContextEJB</ejb-class>
            <session-type>Stateful</session-type>
            <transaction-type>Bean</transaction-type>
        </session>
    </enterprise-beans>
</ejb-jar>
