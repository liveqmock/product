public class WFRouteDecision extends WFAbstractRoute {

    protected String expression;

    public WFRouteDecision() {

    }

    public WFRouteDecision(WFProcess process) {
        super(process);
    }

    /**
     * The expression of this decision;
     *
     * @return the expression;
     */
    public String getExpression() {
        return this.expression;
    }

    /**
     * set the expression of this decision;
     *
     * @param exp
     */
    public void setExpression(String exp) {
        this.expression = exp;
    }
}