html, body {
	font:normal 12px verdana;
	margin:0;
	padding:0;
	border:0 none;
	overflow: auto;
	height:100%;
	background-color: #CAE7E9;
}
#top_bg{background: url(../images/top-bg.gif) repeat-x;height:65px; text-align:left;}
#login-info{height:24px;padding:6px 0pt 0pt 0;background: url(../images/main-toolbar-bg.gif) repeat-x left top;}
#login-info div{float:right;margin:0pt;padding:0pt 5px 0px 10px; text-align:right;}
*html #login-info div.login-user{padding-top:2px;}


