#left-tree{background-color: #eeeeee;height: 100%}
#tree-top-icon{height: 15px;position:0 180px;width: 15px;}
#left-tree-top{overflow:auto;zoom:1;color:#fff;font:bold 11px tahoma,arial,verdana,sans-serif;padding:5px 3px 4px 5px;border:1px solid #76c6cc;line-height:15px;background:transparent url(../images/white-top-bottom.gif) repeat-x 0 -1px;}
#left-tree-icon{width:15px;height:15px;float:right;background:url(../images/tool-sprites.gif) no-repeat;background-position: 0 -195px;}

.left-tree-f{overflow:auto;zoom:1;color:#fff;font:bold 11px tahoma,arial,verdana,sans-serif;padding:5px 3px 4px 5px;border:1px solid #76c6cc;line-height:15px;background:transparent url(../images/top-bg.gif) repeat-x 0 -1px;}